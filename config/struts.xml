<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="kzdefault" namespace="/html" extends="struts-default">
		<action name="sale_*" class="salChanceAction" method="{1}">
			<result name="list_error">~sale/index.jsp</result>
			<result name="list_success">~sale/list.jsp</result>
			<result name="lislTemplate">~sale/listTemplate.jsp</result>
		</action>
		<action name="user_*" class="sysUserAction" method="{1}">
			<result name="login_success">index.jsp</result>
		</action>
		
		<action name="role_*" class="sysRoleAction" method="{1}">
			<result name="list_success">~right/role/list.jsp</result>
		</action>
	</package>
</struts>
