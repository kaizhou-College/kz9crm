<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="default" extends="jfreechart-default">
		<action name="reportAction" class="studentAction">
			<result type="chart">
				<param name="width">400</param>
				<param name="height">300</param>
			</result>
		</action>
		
		<action name="reportBarPic" class="studentAction" method="reportBarPic">
			<result type="chart">
				<param name="width">400</param>
				<param name="height">300</param>
			</result>
		</action>
		<action name="reportActionView" class="studentAction" method="executePie">
		</action>		
	</package>
</struts>
